module TinyGQL
  module Visitors
    module Visitor
<%- nodes.each do |node| -%>
      def handle_<%= node.human_name %> obj
      <%- node.fields.find_all(&:visitable?).each do |field| -%>
        <%- if field.nullable? -%>
        if obj.<%= field.name %>
        <%- end -%>
        <%- if field.list? -%>
            obj.<%= field.name %>.each { _1.accept self }
        <%- end -%>
        <%- if field.node? -%>
            obj.<%= field.name %>.accept self
        <%- end -%>
        <%- if field.nullable? -%>
        end
        <%- end -%>
      <%- end -%>
      end
<%- end -%>
    end

    module Fold
<%- nodes.each do |node| -%>
      def handle_<%= node.human_name %> obj, seed
      <%- node.fields.find_all(&:visitable?).each do |field| -%>
        <%- if field.nullable? -%>
        if obj.<%= field.name %>
        <%- end -%>
        <%- if field.list? -%>
            obj.<%= field.name %>.each { seed = _1.fold(self, seed) }
        <%- end -%>
        <%- if field.node? -%>
            seed = obj.<%= field.name %>.fold self, seed
        <%- end -%>
        <%- if field.nullable? -%>
        end
        <%- end -%>
      <%- end -%>
        seed
      end
<%- end -%>
    end
  end
end
